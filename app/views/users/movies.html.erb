<h1>Viewing Party Lite</h1>

<h2>Top Rated Movies</h2>

<%= button_to 'Discover Page', "/users/#{@user.id}/discover", method: :get %>

<table>
<% @movie_data.each do |movie| %>
    <% movie[:results].each do |m| %>
    <tr>
    <td><%= link_to m[:original_title], "/users/#{@user.id}/movies/#{m[:id]}" %></td>
    <td>Vote Average: <%= m[:vote_average] %></td>
    </tr>
    <% end%>
<% end %>
</table>