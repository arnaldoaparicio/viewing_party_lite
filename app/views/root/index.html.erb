<h1>Viewing Party</h1>
<% if current_user %>
<%= link_to 'Logout', '/logout', method: :delete %>
<% else %>
<%= link_to 'Login', '/login', method: :get %>
<%= button_to 'Create a New User', "/register", method: :get %>
<% end %>

<% if current_user %>
<div class="existing-users">
<% @users.each do |user| %>
  <p><%= user.email %></p>
<% end %>
<% end %>
</div>
